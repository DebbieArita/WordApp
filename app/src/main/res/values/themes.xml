<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RoomWordsSample2" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2196f3</item>
        <item name="colorPrimaryVariant">#64b5f6</item>
        <item name="colorOnPrimary">#1976D2</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FFFF9800</item>
        <item name="colorSecondaryVariant">@android:color/white</item>
        <item name="colorOnSecondary">#fff3e0</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.RoomWordsSample2.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RoomWordsSample.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RoomWordsSample.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>